@model UsersViewModel

@section Headers{
    <breadcrumb></breadcrumb>
}

<table class="table table-striped table-bordered table-hover table-condensed">
    <thead>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>e-mail</th>
    </tr>
    </thead>
    @foreach (var cat in Model.Users)
    {
        <tr>
            <td>@cat.Id</td>
            <td>@cat.UserName</td>
            <td>@cat.Email</td>
        </tr>
    }
</table>